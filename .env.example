# LLM Configuration
LLM_PROVIDER=openai
FAST_LLM_MODEL=gpt-4o-mini
SMART_LLM_MODEL=gpt-4o
STRATEGIC_LLM_MODEL=gpt-4o

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Search Configuration
SEARCH_API=duckduckgo
GOOGLE_API_KEY=your_google_search_api_key_here
GOOGLE_CX_ID=your_google_custom_search_engine_id_here
BING_API_KEY=your_bing_api_key_here
SERPAPI_API_KEY=your_serpapi_key_here

# Research Configuration
MAX_SEARCH_RESULTS_PER_QUERY=8
MAX_ITERATIONS=3
AGENT_ROLE=

# Report Configuration
MAX_SUBTOPICS=5
REPORT_FORMAT=markdown
MAX_SECTIONS=5

# Output Configuration
OUTPUT_PATH=./outputs

# Browser Configuration
SELENIUM_WEB_BROWSER=chrome
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/119.0.0.0 Safari/537.36

# Memory Configuration
MEMORY_BACKEND=local

# Embedding Configuration
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_DIMENSION=1536

# Milvus Configuration
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_COLLECTION_NAME=research_documents
MILVUS_USER=
MILVUS_PASSWORD=

# RAG Configuration
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
MAX_CHUNKS_PER_DOC=100
SIMILARITY_THRESHOLD=0.7
TOP_K_RETRIEVAL=10

# Document Processing Configuration
DOC_PATH=./my-docs
RETRIEVER=web
ENABLE_OCR=false
SUPPORTED_FORMATS=pdf,docx,txt,md,csv,xlsx,json,xml

# Data Stream Configuration
STREAM_BATCH_SIZE=100
STREAM_PROCESSING_INTERVAL=60

# Other Configuration
VERBOSE=true

# LangSmith (Optional - for debugging and monitoring)
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=gpt-researcher-langgraph
